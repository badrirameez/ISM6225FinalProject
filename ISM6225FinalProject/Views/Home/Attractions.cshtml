@model ISM6225FinalProject.Models.Places
<h1>Tampa Attractions</h1>
<hr />
<table width="100%" height="100%" border="0" border-style: solid;>
    <tbody>
        <tr>
            <th>Icon</th>
            <th>Attaction Name</th>
            <th>Address</th>
            <th>Rating Count</th>
            <th>Rating</th>
        </tr>
            @foreach (ISM6225FinalProject.Models.Place p in Model.results)
            {
                <tr>
                    <td align="left">
                        <img src="@p.icon" />
                    </td>
                    <td align="left">
                        @p.name
                    </td>
                    <td align="left">
                        @p.formatted_address
                    </td>
                    <td align="left">
                        @p.user_ratings_total
                    </td>
                    <td align="left">
                        @p.rating
                    </td>

                </tr>
            }


    </tbody>
</table>
